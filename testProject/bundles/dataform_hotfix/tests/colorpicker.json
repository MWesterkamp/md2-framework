{
    "form": {
        "dataform-version": "1.0.0",
        "size": {
            "h": 300,
            "w": 500
        },
        "children": [
            {
                "type": "colorpicker",
                "field": "colorValue1",
                "animatePoint": true,
                "showHsv": true,
                "webSafe": false,
                "showRgb": true,
                "size": {
                    "l": 5,
                    "t": 0
                }
            },
            {
                "type": "label",
                "value": "Color Value: ",
                "size": {
                    "l": 5,
                    "t": 160,
                    "w": 200
                }
            },
            {
                "type": "label",
                "field": "colorValue1",
                "size": {
                    "l": 100,
                    "t": 160,
                    "w": 200
                }
            },
            {
                "type": "colorpicker",
                "field": "colorValue2",
                "animatePoint": true,
                "showHsv": true,
                "webSafe": false,
                "showRgb": true,
                "asDropDown": true,
                "size": {
                    "l": 5,
                    "t": 200
                }
            }
        ]
    },
    "input": {
        "colorValue1" : "#000000",
        "colorValue2" : "#000000"
    }
}