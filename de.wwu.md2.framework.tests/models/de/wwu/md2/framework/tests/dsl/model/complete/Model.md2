package models

entity Book {
	title: string
	isbn: string
	author: string
	copies: Copy[]
}

entity Copy {
	id: integer
	status: Status
	inPortfolioSince: date
	borrowedBy: Person
}

entity Person {
	ename: string
	loans: Copy[]
	address: string
	{
		name "PrivateAddress"
		description "Address only as String!"
	}
}

enum Status {
	"borrowed",
	"reserved",
	"in shelf"
}