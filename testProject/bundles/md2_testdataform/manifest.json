{
    "Bundle-SymbolicName": "md2_testdataform",
    "Bundle-Version": "1.0",
    "Bundle-Name": "TestDataForm",
    "Bundle-Localization": [],
    "Bundle-Main": "",
    "Require-Bundle": [],
    "Components": [{
            "name": "DataFormTest",
            "impl": "ct/Stateful",
            "provides": ["md2.app.AppDefinition"],
            "propertiesConstructor": true,
            "properties": {
                "windowTitle": "Third Party Insurance",
                "serviceUri": "http://localhost:8080/testProject.backend/service/",
                "onInitialized": "startAction",
                "contentProviders": [{
                    "name": "customerProvider",
                    "configuration": {
                        "type": "remote",
                        "entityName": "customer",
                        "serviceUri": "http://localhost:8080/testProject.backend/service/",
                        "filter": {
                            "count": "first",
                            "query": {
                                "customerId": { "$lt": 50 },
                                "firstName": "@startView$outerPanel$firstName"
                            }
                        }
                    }
                }],
                "entities": {
                    "customer": {
                        "string|firstName": "John",
                        "string|lastName": "Doe",
                        "integer|customerId": 7645376,
                        "date|dateOfBirth": "1957-03-17"
                    }
                },
                "views": [{
                    "name": "startView",
                    "dataForm": {
                        "dataform-version": "1.0.0",
                        "size": {
                            "h": 200,
                            "w": 500
                        },
                        "type": "gridpanel",
                        "showLabels": true,
                        "cols": 1,
                        "children": [
                            {
                                "type": "textbox",
                                "datatype": "string",
                                "title": "First Name",
                                "field": "startView$outerPanel$firstName",
                                "max": 50
                            },
                            {
                                "type": "textbox",
                                "datatype": "string",
                                "title": "Last Name",
                                "field": "startView$outerPanel$lastName",
                                "max": 50
                            },
                            {
                                "type": "textbox",
                                "datatype": "string",
                                "title": "Customer Id",
                                "field": "startView$outerPanel$customerId",
                                "regex": "[0-9]{7}$"
                            },
                            {
                                "type": "datetextbox",
                                "datatype": "date",
                                "title": "Date Of Birth",
                                "field": "startView$outerPanel$dateOfBirth",
                                "max": "2005-01-01"
                            },
                            {
                                "type": "checkbox",
                                "datatype": "boolean",
                                "title": "Please check to accept conditions",
                                "field": "startView$outerPanel$acceptConditions"
                            },
                            {
                                "type": "textoutput",
                                "datatype": "string",
                                "field": "startView$outerPanel$juhu",
                                "defaultText": "This is a label!!"
                            }
                        ]
                    }
                },
                {
                    "name": "anotherView",
                    "dataForm": {
                        "dataform-version": "1.0.0",
                        "size": {
                            "h": 300,
                            "w": 500
                        },
                        "type": "md2gridpanel",
                        "cols": 3,
                        "children": [
                            {
                                "type": "textoutput",
                                "datatype": "string",
                                "field": "anotherView$firstNameLabel",
                                "defaultText": "First Name",
                                "cellStyle": {
                                    "width": "33.333%"
                                }
                            },
                            {
                                "type": "textbox",
                                "datatype": "string",
                                "field": "anotherView$firstName",
                                "max": 50,
                                "cellStyle": {
                                    "width": "43.333%"
                                }
                            },
                            {
                                "type": "spacer",
                                "cellStyle": {
                                    "width": "23.333%"
                                }
                            },
                            {
                                "type": "textoutput",
                                "datatype": "string",
                                "field": "anotherView$lastNameLabel",
                                "defaultText": "Last Name",
                                "cellStyle": {
                                    "width": "33.333%"
                                }
                            },
                            {
                                "type": "textbox",
                                "datatype": "string",
                                "field": "anotherView$lastName",
                                "max": 50,
                                "required": true,
                                "cellStyle": {
                                    "width": "43.333%"
                                }
                            },
                            {
                                "type": "spacer",
                                "cellStyle": {
                                    "width": "23.333%"
                                }
                            },
                            {
                                "type": "textoutput",
                                "datatype": "string",
                                "field": "anotherView$customerIdLabel",
                                "defaultText": "Customer ID",
                                "cellStyle": {
                                    "width": "33.333%"
                                }
                            },
                            {
                                "type": "textbox",
                                "datatype": "string",
                                "field": "anotherView$customerId",
                                "regex": "[0-9]{7}$",
                                "cellStyle": {
                                    "width": "43.333%"
                                }
                            },
                            {
                                "type": "spacer",
                                "cellStyle": {
                                    "width": "23.333%"
                                }
                            },
                            {
                                "type": "textoutput",
                                "datatype": "string",
                                "field": "anotherView$dateOfBirthLabel",
                                "defaultText": "Date Of Birth",
                                "cellStyle": {
                                    "width": "33.333%"
                                }
                            },
                            {
                                "type": "datetextbox",
                                "datatype": "date",
                                "field": "anotherView$dateOfBirth",
                                "max": "2005-01-01",
                                "cellStyle": {
                                    "width": "43.333%"
                                }
                            },
                            {
                                "type": "spacer",
                                "cellStyle": {
                                    "width": "23.333%"
                                }
                            },
                            {
                                "type": "spacer",
                                "cellStyle": {
                                    "width": "33.333%"
                                }
                            },
                            {
                                "type": "spacer",
                                "cellStyle": {
                                    "width": "33.333%"
                                }
                            },
                            {
                                "type": "spacer",
                                "cellStyle": {
                                    "width": "33.333%"
                                }
                            },
                            {
                                "type": "spacer",
                                "cellStyle": {
                                    "width": "33.333%"
                                }
                            },
                            {
                                "type": "checkbox",
                                "datatype": "boolean",
                                "field": "anotherView$acceptConditions",
                                "cellStyle": {
                                    "width": "7%"
                                }
                            },
                            {
                                "type": "textoutput",
                                "datatype": "string",
                                "field": "anotherView$acceptConditionsLabel",
                                "defaultText": "Please check to accept conditions",
                                "cellStyle": {
                                    "width": "59.667%"
                                }
                            },
                            {
                                "type": "spacer",
                                "cellStyle": {
                                    "width": "33.333%"
                                }
                            },
                            {
                                "type": "spacer",
                                "cellStyle": {
                                    "width": "33.333%"
                                }
                            },
                            {
                                "type": "spacer",
                                "cellStyle": {
                                    "width": "33.333%"
                                }
                            },
                            {
                                "type": "spacer",
                                "cellStyle": {
                                    "width": "75%"
                                }
                            },
                            {
                                "type": "button",
                                "title": "Save",
                                "field": "anotherView$button",
                                "cellStyle": {
                                    "width": "23%"
                                }
                            },
                            {
                                "type": "spacer",
                                "cellStyle": {
                                    "width": "2%"
                                }
                            }
                        ]
                    }
                }]
            }
        },
        {
            "name": "StartAction",
            "impl": "./actions/StartAction",
            "provides": ["md2.app.CustomAction"]
        },
        {
            "name": "MyConnectionIsGone",
            "impl": "./actions/MyConnectionIsGone",
            "provides": ["md2.app.CustomAction"]
        }
    ]
}
